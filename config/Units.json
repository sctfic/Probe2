{
    "temperature": {
      "unit": "°C", 
      "avaible_units": [{"title":"Celsius","symbol":"°C"}, {"title": "Fahrenheit","symbol":"°F"}, {"title":"Kelvin","symbol":"K"}],
      "sensors": ["THSW", "inside temperature", "outside temperature", "dewPoint", "windChill", "heatIndex", "soil temperature", "leaf temperature"]
    },
    "speed": {
      "unit": "km/h", 
      "avaible_units": [{"title":"mph","symbol":"mph"}, {"title":"m/s","symbol":"m/s"}, {"title":"km/h","symbol":"km/h"}, {"title":"knots","symbol":"knots"}],
      "sensors": ["windSpeed", "windGust"]
    },
    "direction": {
      "unit": "cardinal", 
      "avaible_units": [{"title":"degrees","symbol":"°"}, {"title":"cardinal","symbol":"cardinal"}],
      "sensors": ["windDirection"]
    },
    "pressure": {
      "unit": "Bar", 
      "avaible_units": [{"title":"imperial","symbol":"inhg"}, {"title":"metric","symbol":"hpa"}, {"title":"metric","symbol":"mb"}, {"title":"Bar","symbol":"Bar"}],
      "sensors": ["Barometer"]
    },
    "rain": {
      "unit": "l/m²",   
      "avaible_units": [{"title":"milimetre","symbol":"mm"}, {"title":"inch","symbol":"in"}, {"title":"litre per square meter","symbol":"l/m²"}],
      "sensors": ["Day rain","month rain","year rain", "storm rain", "day ET", "month ET", "year ET"]
    },
    "rainRate": {
      "unit": "l/m²/h",   
      "avaible_units": [{"title":"milimetre per hour","symbol":"mm/h"}, {"title":"inch per hour","symbol":"in/h"}, {"title":"litre per square meter per hour","symbol":"l/m²/h"}],
      "sensors": ["rainRate"]
    },
    "uv": {
      "unit": "index",
      "avaible_units": [{"title":"index","symbol":"index"}, {"title":"temps d'exposition maximum","symbol":"min"}],
      "sensors": ["uvIndex"]
    },
    "powerRadiation": {
      "unit": "w/m²",  
      "avaible_units": [{"title":"watt per square meter","symbol":"w/m²"}],
      "sensors": ["Solar Radiation"]
    },
    "humidity": {
      "unit": "%", 
      "avaible_units": [{"title":"percent","symbol":"%"}],
      "sensors": ["inside humidity","outside humidity"]
    },
    "battery": {
      "unit": "%",
      "avaible_units": [{"title":"percent","symbol":"%"}, {"title":"volts","symbol":"V"}],
      "sensors": ["Console Battery Voltage"]
    },
    "date": {
      "unit": "date",
      "avaible_units": [{"title":"date","symbol":""}],
      "sensors": ["dateStormRain"]
    },
    "time": {
      "unit": "time",
      "avaible_units": [{"title":"time","symbol":""}],
      "sensors": ["sunrise", "sunset", "moonrise", "moonset"]
    },
    "Forecast": {
      "unit": "ForecastClass",
      "avaible_units": [{"title":"ForecastNum","symbol":""}, {"title":"ForecastClass","symbol":""}],
      "sensors": ["ForecastIcons"]
    }
}